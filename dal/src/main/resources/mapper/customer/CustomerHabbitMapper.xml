<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farst.customer.mapper.CustomerLabelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.farst.customer.entity.CustomerHabbit">
        <result column="id" property="id" />
        <result column="habbit_name" property="habbitName" />
        <result column="customer_info_id" property="customerInfoId" />
        <result column="clockin_label_id" property="clockinLabelId" />
