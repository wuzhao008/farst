<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farst.customer.mapper.CustomerInfoMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.farst.customer.entity.CustomerInfo">
        <result column="id" property="id" />
        <result column="phone_number" property="phoneNumber" />
        <result column="password" property="password" />
        <result column="avatar_url" property="avatarUrl" />
        <result column="level" property="level" />
        <result column="sex" property="sex" />
        <result column="nick_name" property="nickName" />
        <result column="birthday" property="birthday" />
        <result column="slogan" property="slogan" />
        <result column="status" property="status" />
        <result column="create_date" property="createDate" />
        <result column="last_edit_time" property="lastEditTime" />
        <result column="last_login_ip" property="lastLoginIp" />
        <result column="last_login_time" property="lastLoginTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        phone_number, password, avatar_url, level, sex, nick_name, birthday, slogan, status, create_date, last_edit_time, last_login_ip, last_login_time
    </sql>

</mapper>
